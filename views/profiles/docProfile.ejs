<div class="container mt-3">
    <div class="row mt-5">
        <div class="col-1"></div>
        <div class="col-10">
            <div class="card mb-3">
                <div class="card-header text-center">Profile</div>
                <div class="card-body">
                    <% data.find((result) => { %>
                    <form method="POST" action="profile/" class="form-horizontal form-material">
                        <div class="form-group">
                            <label class="col-md-12">First Name</label>
                            <div class="col-md-12">
                                <input name="name" id="name" type="text" value="<%=result.name%>" class="form-control form-control-line">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="example-email" class="col-md-12">Email</label>
                            <div class="col-md-12">
                                <input type="email" name="email" id="email" value="<%=result.email%>" class="form-control form-control-line"
                                       name="example-email" id="example-email"> </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-12">Doctor ID </label>
                            <div class="col-md-12">
                                <input type="text" name="id" id="id" value="<%=result.id%>" class="form-control form-control-line">
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-md-12">Phone No</label>
                            <div class="col-md-12">
                                <input type="text" name="phone_no" id="phone_no" value="<%=result.phone_no%>" class="form-control form-control-line">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-12">Address</label>
                            <div class="col-md-12">
                                <input type="text" name="address" id="address" value="<%=result.address%>" class="form-control form-control-line">
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-md-12">Type of Doctor</label>
                            <div class="col-md-12">
                                <input type="text" name="typeOfDoctor" id="typeOfDoctor" value="<%=result.typeOfDoctor%>"
                                       class="form-control form-control-line">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-12">
                                <button type="submit" class="btn btn-success">Update Profile</button>
                            </div>
                        </div>

                    </form>
                    <%}); %>

                </div>
            </div>
        </div>
        <div class="col-1"></div>

        </div>
    </div>
</div>