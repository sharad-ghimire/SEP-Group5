<div class="row">
    <div class="col-1"></div>
    <div class="col-10">
        <% studentData.find((result) => { %>
        <div class="card mt-5 mb-3">
            <div class="card-header">Edit Profile</div>
            <form method="POST" action="profile/" class="form-horizontal form-material mt-3">
                <div class="form-group">
                    <label class="col-md-12">First Name</label>
                    <div class="col-md-12">
                        <input name="name" id="name" type="text" value="<%=result.name%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <label for="example-email" class="col-md-12">Email</label>
                    <div class="col-md-12">
                        <input type="email" name="email" id="email" value="<%=result.email%>" class="form-control form-control-line"
                               name="example-email" id="example-email"> </div>
                </div>
                <div class="form-group">
                    <label class="col-md-12">Student ID </label>
                    <div class="col-md-12">
                        <input type="text" name="stdId" id="stdId" value="<%=result.stdId%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-12">Phone No</label>
                    <div class="col-md-12">
                        <input type="text" name="phone_no" id="phone_no" value="<%=result.phone_no%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-12">Address</label>
                    <div class="col-md-12">
                        <input type="text" name="address" id="address" value="<%=result.address%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-12">Year</label>
                    <div class="col-md-12">
                        <input type="text" name="year" id="year" value="<%=result.year%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-md-12">Semester</label>
                    <div class="col-md-12">
                        <input type="text" name="semester" id="semester" value="<%=result.semester%>" class="form-control form-control-line">
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-12">
                        <button type="submit" class="btn btn-success btn-block">Update Profile</button>
                    </div>
                </div>

            </form>
        </div>
        <%}); %>
    </div>
    <div class="col-1"></div>
</div>